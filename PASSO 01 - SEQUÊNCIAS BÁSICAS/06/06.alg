algoritmo "06"
// Função :
// Autor :
// Data : 20/02/2021
// Seção de Declarações 
var
   numero, At, Su: Inteiro

inicio
      Escreva("Digite um numero: ")
      Leia(numero)
      At <- numero - 1
      Su <- numero + 1
      Escreval("O antecessor de", numero," eh", At)
      Escreva("O sucessor de", numero," eh", Su)


fimalgoritmo
