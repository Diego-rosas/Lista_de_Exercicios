algoritmo "05"
// Função :
// Autor :
// Data : 20/02/2021
// Seção de Declarações 
var
   nota1, nota2, media: Real

inicio
      Escreva("Didite a nota 1: ")
      Leia(nota1)
      Escreva("Digite a nota 2: ")
      Leia(nota2)
      media <- (nota1 + nota2)/2
      Escreva("A media entre",nota1," e",nota2," eh igual a",media)


fimalgoritmo
