algoritmo "04"
// Função :
// Autor :
// Data : 20/02/2021
// Seção de Declarações 
var
     N1, N2, S: Inteiro
inicio
      Escreva("Digite um numero: ")
      Leia(N1)
      Escreva("Digite outro numero: ")
      Leia(N2)
      S <- N1 + N2
      Escreva("A soma entre",N1," e",N2," eh igual a",S)

fimalgoritmo
